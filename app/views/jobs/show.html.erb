            <%= label :job, :description, 'Job Description' %>
            <%= text_field_tag :job_description, job[:JobDescription], class: "form-control", readonly: true %>
        </div>
    </div>
    <div class="form-row">
        <div class="form-group  col-md-12">
            <%= label :job, :comment, 'Comment'  %>
            <%= text_field_tag :comment, job[:Comment], class: "form-control", readonly: true %>
        </div>
  </div>

